<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="fill_horizontal|center" android:id="@android:id/background">
        <shape android:shape="rectangle">
            <solid android:color="@color/miuix_appcompat_progress_background_light" />
            <corners android:radius="@dimen/miuix_appcompat_seekbar_progress_bg_radius" />
        </shape>
    </item>
    <item android:gravity="fill_horizontal|center" android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <solid android:color="@color/miuix_appcompat_progress_primary_colors_light" />
                <corners android:radius="@dimen/miuix_appcompat_seekbar_progress_bg_radius" />
            </shape>
        </clip>
    </item>
</layer-list>